{
  "name": "vue-hook-optimizer",
  "version": "0.0.41",
  "description": "a tool that helps refactor and optimize hook abstractions in Vue components",
  "packageManager": "pnpm@8.6.12",
  "homepage": "https://github.com/zcf0508/vue-hook-optimizer",
  "repository": {
    "type": "git",
    "url": "https://github.com/zcf0508/vue-hook-optimizer.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nodemon",
    "dev:ext": "npm -C packages/vscode run dev",
    "build": "tsup",
    "build:ext": "npm -C packages/vscode run build",
    "play": "npm -C packages/playground run dev",
    "lint": "eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "release": "bumpp -r",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "hooks"
  ],
  "author": "zcf0508 <zcf0508@live.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/parser": "7.22.5",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@types/babel__core": "^7.20.1",
    "@types/babel__traverse": "7.20.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.8",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vue/compiler-sfc": "^3.3.4",
    "acorn": "^8.11.2",
    "acorn-walk": "^8.3.0",
    "bumpp": "^9.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "lodash-es": "^4.17.21",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "vis-network": "^9.1.6",
    "vitest": "^0.34.1",
    "vue-eslint-parser": "^9.3.1"
  }
}
